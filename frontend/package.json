{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier --write \"src/**/*.{ts,html,scss}\" --config .prettierrc",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss}\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "api": "openapi-generator-cli generate -g typescript-angular -i ../backend/swagger-spec.json -o ./src/app/api && npm run format",
    "licenses": "license-checker-rseidelsohn --production --onlyAllow=\"BlueOak-1.0.0;MIT;BSD-3-Clause;Apache-2.0;ISC;Python-2.0;UNLICENSED;CC-BY-4.0;BSD-2-Clause;Apache*;0BSD\" --json --out=\"./licenses.json\" --customPath customFormat.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.6",
    "@angular/cdk": "^17.0.3",
    "@angular/common": "^17.0.6",
    "@angular/compiler": "^17.0.6",
    "@angular/core": "^17.0.6",
    "@angular/forms": "^17.0.6",
    "@angular/material": "^17.0.3",
    "@angular/platform-browser": "^17.0.6",
    "@angular/platform-browser-dynamic": "^17.0.6",
    "@angular/router": "^17.0.6",
    "@fontsource/roboto": "^5.0.8",
    "@ngneat/transloco": "^6.0.4",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngxs/logger-plugin": "^3.8.2",
    "@ngxs/store": "^3.8.2",
    "isbn3": "^1.1.43",
    "jsbarcode": "^3.11.6",
    "material-icons": "^1.13.12",
    "ng-lazyload-image": "^9.1.3",
    "ng-recaptcha": "^13.2.1",
    "ngx-timeago": "^3.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "wrap-ansi": "^8.1.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular/cli": "~17.0.7",
    "@angular/compiler-cli": "^17.0.6",
    "@ngxs/devtools-plugin": "^3.8.2",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "license-checker-rseidelsohn": "^4.3.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.2.0"
